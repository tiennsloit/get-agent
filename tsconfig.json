{
	"compilerOptions": {
		"module": "Node16",
		"target": "ES2022",
		"outDir": "dist",
		"lib": [
			"ES2022"
		],
		"sourceMap": false,
		"rootDir": "src",
		"strict": true,
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"skipLibCheck": true
	},
	"exclude": [
		"node_modules",
		"dist",
		"webviews",
		"functions"
	]
}