{
  "name": "gonext",
  "publisher": "gonext",
  "displayName": "GoNext",
  "description": "AI-powered code assistance for VSCode",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.99.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tiennsloit/gonext"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "walkthroughs": [
      {
        "id": "gonext-intro",
        "title": "Introduction to GoNext",
        "description": "Let's get started with GoNext - Your AI-powered Flow Management Assistant!",
        "steps": [
          {
            "id": "welcome",
            "title": "Welcome",
            "description": "Welcome to GoNext! Your AI-powered Flow Management assistant for VSCode.",
            "media": {
              "markdown": "assets/walkthrough/welcome.md"
            }
          },
          {
            "id": "setup",
            "title": "Setup environment",
            "description": "Click the button below to setup your environment.\n[Setup](command:gonext.setup-environment)",
            "media": {
              "image": "assets/walkthrough/placeholder.svg",
              "altText": "Empty image"
            },
            "completionEvents": [
              "onCommand:gonext.setup-environment"
            ]
          },
          {
            "id": "flow-management",
            "title": "Flow Management",
            "description": "Use GoNext to create and execute complex development workflows.",
            "media": {
              "markdown": "assets/walkthrough/welcome.md"
            }
          }
        ]
      }
    ],
    "configuration": {
      "title": "GoNext Settings",
      "properties": {
        "gonext.ai.provider": {
          "type": "string",
          "default": "openai",
          "description": "AI provider identifier"
        },
        "gonext.ai.apiKey": {
          "type": "string",
          "default": "",
          "description": "API key for the AI provider"
        },
        "gonext.ai.baseUrl": {
          "type": "string",
          "default": "https://api.openai.com/v1",
          "description": "Base URL for the AI provider API"
        },
        "gonext.api.baseUrl": {
          "type": "string",
          "default": "https://gonext.hien.one/api",
          "description": "Base URL for GoNext API requests"
        },
        "gonext.api.timeout": {
          "type": "number",
          "default": 30000,
          "description": "Default request timeout in milliseconds"
        },
        "gonext.api.retryLimit": {
          "type": "number",
          "default": 2,
          "description": "Maximum number of retry attempts for failed requests"
        }
      }
    },
    "commands": [
      {
        "command": "gonext.show-settings",
        "title": "GoNext: Open Settings"
      },
      {
        "command": "gonext.show-sidebar",
        "title": "GoNext: Open Sidebar"
      },
      {
        "command": "gonext.show-walkthrough",
        "title": "GoNext: Show Walkthrough"
      },
      {
        "command": "gonext.setup-environment",
        "title": "GoNext: Setup Environment"
      }
    ],
    "keybindings": [],
    "menus": {
      "view/title": []
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "gonext-sidebar",
          "title": "GoNext",
          "icon": "assets/icon.svg"
        }
      ]
    },
    "views": {
      "gonext-sidebar": [
        {
          "id": "gonext.sidebar",
          "name": "Flow Management",
          "type": "webview",
          "icon": "assets/icon.svg",
          "visibility": "visible"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "start:sidebar": "cd webviews/sidebar && npm run dev",
    "build:sidebar": "cd webviews/sidebar && npm run build",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/lodash": "^4.17.16",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.99.1",
    "@typescript-eslint/eslint-plugin": "^8.25.0",
    "@typescript-eslint/parser": "^8.25.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.21.0",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "got": "^14.4.7",
    "ignore": "^7.0.3",
    "inversify": "^7.5.1",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "openai": "^5.3.0",
    "uuid": "^11.1.0",
    "web-tree-sitter": "^0.25.3"
  },
  "extensionDependencies": []
}
